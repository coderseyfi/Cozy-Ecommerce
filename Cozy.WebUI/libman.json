{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery-cookie@1.4.1",
      "destination": "wwwroot/libs/jquery-cookie/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/libs/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "simplebar@5.3.8",
      "destination": "wwwroot/libs/simplebar/",
      "files": [
        "simplebar.min.js",
        "simplebar.min.css"
      ]
    },
    {
      "library": "imask@6.4.3",
      "destination": "wwwroot/libs/imask/",
      "files": [
        "imask.min.js"
      ]
    }
  ]
}