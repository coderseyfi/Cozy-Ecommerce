
@{
    ViewData["Title"] = "GetLikes";
}

@model IEnumerable<BlogPostLike>

@{
    ViewData["Title"] = "Comments";
}

<div class="page-header">
    <div class="row align-items-end">

        <div class="col">
            <div class="page-header-breadcrumb">
                <ul class="breadcrumb-title">
                    <li class="breadcrumb-item" style="float: left;">
                        <a asp-controller="Dashboard" asp-action="index"> <i class="feather icon-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item" style="float: left;">
                        <a asp-action="index">All blogposts</a>
                    </li>
                    <li class="breadcrumb-item" style="float: left;">
                        <span>All liked users</span>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>


<!-- Page-body start -->
<div class="page-body">


    <!-- Horizontal-border table start -->
    <div class="card">
        <div class="card-block table-border-style">
            <div class="table-responsive">
                <table class="table table-framed table-imaged">

                    <thead>
                        <tr>
                            <th>
                                Author
                            </th>
                            <th>
                                Liked date
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.CreatedByUser?.Name @item.CreatedByUser?.Surname
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CreatedDate)
                                </td>
                            </tr>

                        }
                    </tbody>

                </table>
            </div>
        </div>
    </div>
    <!-- Horizontal-border table end -->
    @Html.AntiForgeryToken()
</div>
<!-- Page-body end -->

